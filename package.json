{
  "name": "pastebin-cf-worker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bash -lc 'set -a; [ -f .dev.vars ] && . .dev.vars; set +a; wrangler dev --local --persist-to .wrangler/state --var TURNSTILE_SITE_KEY=$TURNSTILE_SITE_KEY --var TURNSTILE_SECRET=$TURNSTILE_SECRET'",
    "dev:remote": "wrangler dev",
    "deploy": "wrangler deploy",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260111.0",
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "typescript-eslint": "^8.52.0",
    "typescript": "^5.7.3"
  }
}
